{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-mailparse": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/fortify": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "sendmarc/dns-lookup": "dev-master",
        "sendmarc/dns-records": "dev-master",
        "sendmarc/domain-analysis": "dev-master",
        "sendmarc/email-analysis": "dev-master",
        "sendmarc/sender-analysis": "dev-master",
        "sendmarc/utilities": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/dns-lookup.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/dns-records.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/domain-analysis.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/utilities.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/sender-analysis.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sendmarc/email-analysis.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi"
        ]
    }
}
